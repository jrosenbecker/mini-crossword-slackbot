{
  "name": "crossword-timer-slackbot",
  "version": "0.0.0",
  "description": "crossword-timer-slackbot",
  "license": "MIT",
  "repository": "",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "serverless offline",
    "deploy": "sls deploy -s prod",
    "debug": "node --inspect ./node_modules/serverless/bin/serverless offline",
    "clean": "rimraf lib && rimraf coverage",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\" --single-quote --trailing-comma es5",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "test": "jest",
    "coverage": "jest --coverage",
    "watch": "npm run build -- --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "aws-lambda": "^0.1.2",
    "aws-serverless-express": "^3.3.5",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dynamoose": "^1.7.2",
    "express": "^4.16.4",
    "left-pad": "^1.3.0",
    "lodash": "^4.17.4",
    "luxon": "^1.11.4",
    "serverless": "^1.39.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@types/aws-serverless-express": "^3.3.0",
    "@types/body-parser": "^1.16.3",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.0.35",
    "@types/jest": "^22.0.1",
    "@types/lodash": "^4.14.65",
    "@types/luxon": "^1.11.1",
    "@types/node": "^8.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-modern-regexp": "0.0.6",
    "coveralls": "^2.0.0",
    "jest": "^22.0.4",
    "prettier": "^1.5.2",
    "rimraf": "^2.0.0",
    "serverless-offline": "^4.9.2",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "^22.0.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^3.2.0",
    "tslint": "^5.0.0",
    "tslint-config-prettier": "^1.1.0",
    "typescript": "^2.3.0",
    "webpack": "^4.29.6"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "jest": {
    "transform": {
      ".(ts)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testEnvironment": "node"
  }
}
